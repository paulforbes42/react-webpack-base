{
    "parser": "@babel/eslint-parser",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:jest/recommended",
        "airbnb"
    ],
    "plugins": [
        "react-hooks"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "react-hooks/rules-of-hooks": "error"
    }
}